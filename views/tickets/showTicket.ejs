<%- include('../partials/navbaruser.ejs') %>
<div class="container">

<h1>TICKETS DETAIL</h1>
<table class="table">
  <thead>
    <th>Ticket</th>
    <th>Price</th> 
    <th>Date</th>
    <th>Options</th>
  </thead>
    <tr>
    <td><%= product.name %></td>
    <td><%= product.price %></td>
    <td><%= product.date %></td>
  </tr>
</table>
<div class="container">
  <form action="" method="POST" class="mb-4 mt-4">
  <div class="form-group">
    <label>Quantity</label>
    <input type="hidden" name="productPrice" value="<%= product.price %>" >
    <input type="text" class="form-control" name="quantity">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</div>